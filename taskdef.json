{
  "family": "my-platform-dev",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "executionRoleArn": "arn:aws:iam::742508967962:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::742508967962:role/my-platform-dev-ecs-task",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "742508967962.dkr.ecr.ap-northeast-2.amazonaws.com/my-sample-api:${{ github.sha }}",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/my-platform-dev",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "app"
        }
      },
      "environment": [
        { "name": "PORT", "value": "8080" }
      
      ]
    }
  ]
}
